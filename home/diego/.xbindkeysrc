"pactl set-sink-volume @DEFAULT_SINK@ +5000"
XF86AudioRaiseVolume

"pactl set-sink-volume @DEFAULT_SINK@ -5000"
XF86AudioLowerVolume

"pactl set-sink-mute @DEFAULT_SINK@ toggle"
XF86AudioMute

"xbacklight -inc 10"
XF86MonBrightnessUp

"xbacklight -dec 10"
XF86MonBrightnessDown

"playerctl play-pause"
XF86AudioPlay

"playerctl stop"
XF86AudioStop

"playerctl next"
XF86AudioNext

"playerctl previous"
XF86AudioPrev
